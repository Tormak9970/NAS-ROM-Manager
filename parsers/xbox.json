{
  "name": "Microsoft Xbox",
  "abbreviation": "Xbox",
  "folder": "xbox",
  "patterns": [
    {
      "glob": "*/(?i)default.xbe",
      "regex": "\\/(?<title>.+)\\/default\\.xbe$",
      "downloadStrategy": {
        "type": "folder",
        "parent": "xbox"
      }
    },
    {
      "glob": "*.(?i)iso",
      "regex": "\\/(?<title>.+)\\.iso$",
      "downloadStrategy": {
        "type": "single-file"
      }
    }
  ]
}