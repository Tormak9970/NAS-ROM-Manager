{
  "name": "Sony PlaySation 2",
  "abbreviation": "PS2",
  "folder": "ps2",
  "patterns": [
    {
      "glob": "*.(?i){7z,bin,chd,ciso,cso,dump,elf,gz,img,iso,m3u,mdf,nrg,zip}",
      "regex": "\\/(?<title>.+)\\.(?:7z|bin|chd|ciso|cso|dump|elf|gz|img|iso|m3u|mdf|nrg|zip)$",
      "downloadStrategy": {
        "type": "single-file"
      }
    }
  ]
}