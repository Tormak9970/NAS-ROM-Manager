{
  "name": "Nintendo WiiU",
  "abbreviation": "WiiU",
  "folder": "wiiu",
  "patterns": [
    {
      "glob": "*/*/*.(?i)rpx",
      "regex": "\\/(?<title>.+)\\/.+\\/.+(?:rpx)$",
      "downloadStrategy": {
        "type": "folder",
        "parent": "wiiu"
      }
    },
    {
      "glob": "*.(?i){wud,wux,wua}",
      "regex": "\\/(?<title>.+)\\.(?:wud|wux|wua)$",
      "downloadStrategy": {
        "type": "single-file"
      }
    }
  ]
}